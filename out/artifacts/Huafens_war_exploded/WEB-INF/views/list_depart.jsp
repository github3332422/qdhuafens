<script type="text/javascript">
    function deleteCustomer(id) {
        if(confirm('确实要删除该客户吗?')) {
            $.post("/delete",{"id":id},
                function(data){
                    if(data =="OK"){
                        alert("客户删除成功！");
                        window.location.reload();
                    }else{
                        alert("删除客户失败！");
                        window.location.reload();
                    }
                });
        }
    }
</script>
